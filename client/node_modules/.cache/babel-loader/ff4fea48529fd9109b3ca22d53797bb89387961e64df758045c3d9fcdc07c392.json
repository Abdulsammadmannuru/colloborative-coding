{"ast":null,"code":"import{v4 as uuidv4}from'uuid';import{SET_ALERT,REMOVE_ALERT}from'./types';export const setAlert=function(msg,alertType){let timeout=arguments.length>2&&arguments[2]!==undefined?arguments[2]:5000;return dispatch=>{const id=uuidv4();dispatch({type:SET_ALERT,payload:{msg,alertType,id}});setTimeout(()=>dispatch({type:REMOVE_ALERT,payload:id}),timeout);};};","map":{"version":3,"names":["v4","uuidv4","SET_ALERT","REMOVE_ALERT","setAlert","msg","alertType","timeout","dispatch","id","type","payload","setTimeout"],"sources":["C:/Users/Bhanuchinne/Desktop/PROJECT-MERN/client/src/actions/alert.js"],"sourcesContent":["import { v4 as uuidv4 } from 'uuid';\n\nimport { SET_ALERT, REMOVE_ALERT } from './types';\n\nexport const setAlert =\n  (msg, alertType, timeout = 5000) =>\n  (dispatch) => {\n    const id = uuidv4();\n\n    dispatch({\n      type: SET_ALERT,\n      payload: { msg, alertType, id },\n    });\n\n    setTimeout(\n      () =>\n        dispatch({\n          type: REMOVE_ALERT,\n          payload: id,\n        }),\n      timeout\n    );\n  };\n"],"mappings":"AAAA,OAASA,EAAE,GAAIC,OAAM,KAAQ,MAAM,CAEnC,OAASC,SAAS,CAAEC,YAAY,KAAQ,SAAS,CAEjD,MAAO,MAAMC,SAAQ,CACnB,SAACC,GAAG,CAAEC,SAAS,KAAEC,QAAO,2DAAG,IAAI,OAC9BC,SAAQ,EAAK,CACZ,KAAMC,GAAE,CAAGR,MAAM,EAAE,CAEnBO,QAAQ,CAAC,CACPE,IAAI,CAAER,SAAS,CACfS,OAAO,CAAE,CAAEN,GAAG,CAAEC,SAAS,CAAEG,EAAG,CAChC,CAAC,CAAC,CAEFG,UAAU,CACR,IACEJ,QAAQ,CAAC,CACPE,IAAI,CAAEP,YAAY,CAClBQ,OAAO,CAAEF,EACX,CAAC,CAAC,CACJF,OAAO,CACR,CACH,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}